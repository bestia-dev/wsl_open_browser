    "version": "0.2.0",
    "configurations": [       
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'wsl_open_browser'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=wsl_open_browser",
                    "--package=wsl_open_browser"
                ],
                "filter": {
                    "name": "wsl_open_browser",
                    "kind": "bin"
                }
            },
            "args": ["docs"],
            "cwd": "${workspaceFolder}"
        }
    ]
}